{% extends "SamplesProgramadoresBundle::base.html.twig" %}{#% set code = code(_self) %#}

{% block title %}Ejemplos DQL{% endblock %}

{% block content%}

<table class="table table-bordered table-hover table-striped">
<thead>
    <th>No</th>
    <th>NOMBRE</th>
    <th>SISTEMA OPERATIVO</th>
    <th>LENGUAJES CONOCIDOS</th>
</thead>
<tbody>
    {% for programador in programadores %}
    <tr>
        <td>{{loop.index}}</td>
        <td>{{programador.nombre}}</td>
        <td>{{programador.sistemaoperativo}}</td>
        <td>
            <ul>
            {% for lenguaje in programador.lenguajesprogramacion %}
                    <li>{{ lenguaje }}</li>
             {% endfor%}        
            </ul>
        
        </td>
    </tr>
    {% endfor%}
</tbody>
</table>

{% endblock %}